// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  password String
}

model PhoneNumber {
  id     Int    @id @default(autoincrement())
  mobile String
}

model CardId {
  id     Int    @id @default(autoincrement())
  cardId String
}

